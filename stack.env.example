HOST=overleaf.example.com
# SHARELATEX_STATUS_PAGE_URL=status.example.com
CLOUDFLARE_TUNNEL_TOKEN=

SHARELATEX_APP_NAME=
SHARELATEX_SITE_URL=https://overleaf.example.com
# SHARELATEX_NAV_TITLE= # defaults to SHARELATEX_APP_NAME
# SHARELATEX_HEADER_IMAGE_URL=/touch-icon-192x192.png
SHARELATEX_ADMIN_EMAIL=overleaf@example.com

# SHARELATEX_LEFT_FOOTER=[{"text": "Support", "url": "mailto:admin@example.com"}]
# SHARELATEX_RIGHT_FOOTER='[{"text": "Overleaf is the best!"} ]'
# SHARELATEX_HEADER_EXTRAS=

ENABLED_LINKED_FILE_TYPES=url,project_file,project_output_file
# ADDITIONAL_TEXT_EXTENSIONS='log'

# Enables Thumbnail generation using ImageMagick
ENABLE_CONVERSIONS=true

ENABLE_CRON_RESOURCE_DELETION: true
COMPILE_TIMEOUT=30 # seconds

SHARELATEX_RESTRICT_INVITES_TO_EXISTING_ACCOUNTS=true
SHARELATEX_ALLOW_PUBLIC_ACCESS=false
SHARELATEX_ALLOW_ANONYMOUS_READ_AND_WRITE_SHARING=false

# SHARELATEX_PASSWORD_VALIDATION_PATTERN="aA$1"
# SHARELATEX_PASSWORD_VALIDATION_MIN_LENGTH=8
# SHARELATEX_PASSWORD_VALIDATION_MAX_LENGTH=72

# Disables email confirmation requirement
EMAIL_CONFIRMATION_DISABLED=false

# temporary fix for LuaLaTex compiles
# see https://github.com/overleaf/overleaf/issues/695
TEXMFVAR=/var/lib/sharelatex/tmp/texmf-var

## Set for TLS via nginx-proxy
SHARELATEX_BEHIND_PROXY=true
SHARELATEX_SECURE_COOKIE=true
# SHARELATEX_TRUSTED_PROXY_IPS # this is set to true if unspecified

# SHARELATEX_PROXY_LEARN=true

# SHARELATEX_FILESTORE_BACKEND=s3
# SHARELATEX_FILESTORE_USER_FILES_BUCKET_NAME=
# SHARELATEX_FILESTORE_TEMPLATE_FILES_BUCKET_NAME=
# SHARELATEX_FILESTORE_S3_ACCESS_KEY_ID=
# SHARELATEX_FILESTORE_S3_SECRET_ACCESS_KEY=
# SHARELATEX_FILESTORE_S3_SECRET_ACCESS_KEY=
# SHARELATEX_FILESTORE_S3_PATH_STYLE=
# SHARELATEX_FILESTORE_S3_REGION=

SHARELATEX_EMAIL_FROM_ADDRESS=overleaf@example.com
SHARELATEX_EMAIL_REPLY_TO=overleaf@example.com

SHARELATEX_EMAIL_DRIVER=ses
SHARELATEX_EMAIL_AWS_SES_ACCESS_KEY_ID=
SHARELATEX_EMAIL_AWS_SES_SECRET_KEY=

# SHARELATEX_EMAIL_SMTP_HOST=email-smtp.us-east-1.amazonaws.com
# SHARELATEX_EMAIL_SMTP_PORT=465
# SHARELATEX_EMAIL_SMTP_SECURE=true
# SHARELATEX_EMAIL_SMTP_USER=
# SHARELATEX_EMAIL_SMTP_PASS=
# SHARELATEX_EMAIL_SMTP_NAME=
# SHARELATEX_EMAIL_SMTP_TLS_REJECT_UNAUTH=true
# SHARELATEX_EMAIL_SMTP_IGNORE_TLS=false

SHARELATEX_EMAIL_SMTP_LOGGER=true
SHARELATEX_CUSTOM_EMAIL_FOOTER=Disclaimer: In no event will the provider of this Overleaf instance be liable for any special, incidental, indirect, or consequential damages whatsoever arising from your access or use of, or inability to access or use, this site.
